@using System.Globalization
@{
    string company_name = "";
    string company_picture = "";
    string staff_name = "";
    string staff_photo = "";

    if (Request.Cookies["opLCx"] != null)
    {
        company_name = Request.Cookies["cOLCx"].Value;
        company_name = company_name.Replace("kdBkLi=", "");

        company_picture = Request.Cookies["cOPLCx"].Value;
        company_picture = company_picture.Replace("kdBkLi=", "");

        staff_name = Request.Cookies["SnLCx"].Value;
        staff_name = staff_name.Replace("kdBkLi=", "");

        staff_photo = Request.Cookies["SpLCx"].Value;
        staff_photo = staff_photo.Replace("kdBkLi=", "");
    }
}

