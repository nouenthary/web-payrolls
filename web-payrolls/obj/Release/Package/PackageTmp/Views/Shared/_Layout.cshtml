    <script type="text/javascript">

        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-36251023-1']);
        _gaq.push(['_setDomainName', 'jqueryscript.net']);
        _gaq.push(['_trackPageview']);

        (function () {
            let ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            let s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();

        $(function () {
            let token = $('input[name="__RequestVerificationToken"]').val();

            function GetNotification() {
                $.ajax({
                    type: "POST",
                    url: "@Url.Action("GetNotification","Notification")",
                    data: {
                        __RequestVerificationToken: token
                    },
                    success: function (response) {
                        let list = '';

                        for (let i = 0; i < response.length; i++) {

                            list +=
                                '<a href="' + response[i].Link + '">' +
                                '<div class="user-img"> <img src="/Content/Uploads/Staff_Photoes/' + response[i].Photo + '" alt="user" class="img-circle"  onerror=this.src=src="/Content/images/avatar.png"> <span class="profile-status online pull-right"></span> </div>' +
                                '<div class="mail-contnet">' +
                                '<h5>' + response[i].Staff_Name + '</h5> <span class="mail-desc">' + response[i].Notification + '</span> <span class="time">' + response[i].Date_Update + " " + response[i].Time_Update + '</span>' +
                                '</div>' +
                                '</a>';
                        }

                        $('#notification').html(list);

                        if (response.length > 0) {
                            $('#label-number').attr('class', 'label-alert')
                            $('#label-number').text(response.length)
                        }
                    }
                });
            }
            GetNotification();

            // change language
            $(document).on('click','div #change-language a',function() {
                $.ajax({
                    type: "POST",
                    url: "/Language/Change",
                    data: {
                        lang :  $(this).attr('id')
                    },
                    success: function(response) {
                       if(response){
                           window.location.reload();
                       }
                    }
                });
            });

            localStorage.setItem('token_link',token + token);

        });

    </script>
