<script type="text/javascript">

  function isDecimalNumber(key) {
    var keycode = (key.which) ? key.which : key.keycode;
    var parts = key.srcElement.value.split('.');

    if (parts.length > 1 && keycode == 46) {
      return false;
    }
    else {
      if (keycode == 46 || keycode >= 48 && keycode <= 57)
        return true;
      return false;
    }
  }

  function AcceptOnlyNumeric(event) {
    var theEvent = event || window.event;
    // Handle paste
    if (theEvent.type === 'paste') {
      key = event.clipboardData.getData('text/plain');
    } else {
      //handle key press
      var key = theEvent.keyCode || theEvent.which;
      key = String.fromCharCode(key);
    }

    var regex = /[0-9]/;
    if (!regex.test(key)) {
      theEvent.returnValue = false;
      if (theEvent.preventDefault) theEvent.preventDefault();
    }
  }

  function checkStrengthPassword() {
    var number = /([0-9])/;
    var alphabets = /([a-zA-Z])/;
    var special_characters = /([~,!,#,$,%,^,&,*,-,_,+,=,?,>,<])/;

    if ($('#txbPassword').val().length < 8) {

      if ($('#txbPassword').val().length == 0) {

        $('#password_strenght_status').removeClass();
        $('#password_strenght_status').html("");
      } else {

        $('#password_strenght_status').removeClass();
        $('#password_strenght_status').addClass('weak_password');
        $('#password_strenght_status').html("Weak (should be at least 8 characters.)");
      }

    } else {
      if ($('#txbPassword').val().match(number) && $('#txbPassword').val().match(alphabets)
        && $('#txbPassword').val().match(special_characters)) {

        $('#password_strenght_status').removeClass();
        $('#password_strenght_status').addClass('strong_password');
        $('#password_strenght_status').html('Strong');
      } else {

        $('#password_strenght_status').removeClass();
        $('#password_strenght_status').addClass('medium_password');
        $('#password_strenght_status').html('Medium (should include alphabets, numbers and special characters.)');
      }
    }
  }
</script>
<script>
  function openTab(evt, cityName) {

    var i, tabcontent, tablinks;
    tabcontent = document.getElementsByClassName("tabcontent");
    for (i = 0; i < tabcontent.length; i++) {
      tabcontent[i].style.display = "none";
    }

    tablinks = document.getElementsByClassName("tablinks");
    for (i = 0; i < tablinks.length; i++) {
      tablinks[i].className = tablinks[i].className.replace(" active", "");
    }

    document.getElementById(cityName).style.display = "block";
    evt.currentTarget.className += " active";
    
  }
  
  // Get the element with id="defaultOpen" and click on it
  document.getElementById("defaultOpen").click();
</script>

